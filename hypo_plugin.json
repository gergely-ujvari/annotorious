{
  "id": "annotorious-hypo-plugin",
  "define": {
    "goog.DEBUG": false,
    "goog.dom.ASSUME_STANDARDS_MODE": true
  },
  "inputs": [
    "src/shape/point.js",
    "src/shape/polygon.js",
    "src/shape/rectangle.js",
    "src/shape/shape.js",
    "src/mediatypes/image/image.annotator.js",
    "src/mediatypes/image/image.viewer.js",
    "src/okfn/hypo_image_plugin.js",
    "src/selection/rect_drag_selector.js",
    "src/annotation.js",
    "src/dom.js",
    "src/events.js",
    "src/events.ui.js",
    "src/hint.js",

    "templates/core_elements.soy",
    "templates/image_elements.soy"
  ],
  "paths": [
    "."
  ],
  "externs": [
    "externs/api.externs.js",
    "externs/okfn_annotator.externs.js",
    "//json.js",
    "//webkit_console.js",
